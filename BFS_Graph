from collections import deque
def bfs(graph, start):
    visited = set()
    queue = deque([start])
    visited.add(start)
    while queue:
        node = queue.popleft()
        print(node, end=" ")
        for neighbor in graph[node]:
            if neighbor not in visited:
                queue.append(neighbor)
                visited.add(neighbor)
graph = {
    0: [1, 2],
    1: [2],
    2: [0, 3],
    3: [3]
}
print("BFS Traversal starting from node 2:")
bfs(graph, 2)
